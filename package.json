    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "concurrently": "^5.3.0",
    "css-loader": "3.2.0",
    "dataloader": "^2.0.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "effector": "21.0.3",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.1",
