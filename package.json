    "react-dev-utils": "^10.0.0",
    "react-dom": "^16.14.0",
    "resolve": "1.12.2",
    "resolve-url-loader": "^3.1.2",
    "rocket-pipes": "^0.9.0",
    "sass-loader": "8.0.0",
    "semver": "6.3.0",
